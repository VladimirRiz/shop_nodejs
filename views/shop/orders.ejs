<%- include('../includes/head.ejs') %>
    </head>
<body>
    <%- include('../includes/nav.ejs') %>
    <main class="main">
        <% if(orders.length <= 0) { %>
            Nothing yet!
        <% } else { %>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1># <%= order._id %></h1>
                        <ul>
                                <% order.items.forEach(product => { %>
                                    <li><%= product.title %> <%= product.quantity %></li>
                               <% }) %>
                        </ul>
                    </li>
              <% }) %>
            </ul>
       <% } %>
    </main>
<%- include('../includes/end.ejs') %>